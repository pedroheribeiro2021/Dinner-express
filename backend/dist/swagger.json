{
    "openapi": "3.0.1",
    "info": {
        "title": "Dinner Express",
        "description": "Documentation from api Dinner Express",
        "version": "1.0.0"
    },
    "basePath": "/",
    "components": {
        "securitySchemes": {
            "BearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    },
    "paths": {
        "/restaurant": {
            "post": {
                "tags": ["Restaurants"],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "cnpj": {
                                        "type": "string"
                                    },
                                    "type": {
                                        "type": "string"
                                    },
                                    "cellPhone": {
                                        "type": "string"
                                    },
                                    "operatingTimes": {
                                        "type": "object",
                                        "properties": {
                                            "dayOfWeek": {
                                                "type": "string"
                                            },
                                            "openingTime": {
                                                "type": "string"
                                            },
                                            "closingTime": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                },
                                "example": {
                                    "name": "McDonalds",
                                    "cnpj": "28.713.241/0001-05",
                                    "type": "lanchonete",
                                    "cellPhone": "61992514712",
                                    "operatingTimes": {
                                        "dayOfWeek": "Segunda",
                                        "openingTime": "08:00:00",
                                        "closingTime": "22:30:00"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "name": "McDonalds",
                        "cnpj": "28.713.241/0001-05",
                        "type": "lanchonete",
                        "cellPhone": "61992514712",
                        "id": "cfc5dd59-3525-4d75-aa70-61ddfa6262cd"
                    },
                    "400": {
                        "description": "CNPJ already registred"
                    }
                }
            }
        },
        "/restaurant-all": {
            "get": {
                "description": "Route get all restaurants from api",
                "tags": ["Restaurants"],
                "responses": {
                    "200": {
                        "id": "cfc5dd59-3525-4d75-aa70-61ddfa6262cd",
                        "name": "Birsokinha maluca",
                        "cnpj": "28.713.241/0001-05",
                        "type": "lanchonete",
                        "cellPhone": "61992514712",
                        "operatingTimes": {
                            "id": "87daecef-b540-4118-b169-f7abee89fd42",
                            "dayOfWeek": "Segunda-",
                            "openingTime": "08:00:00",
                            "closingTime": "22:30:00"
                        }
                    }
                }
            }
        },
        "/restaurant/{id}": {
            "patch": {
                "tags": ["Restaurants"],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "cnpj": {
                                        "type": "string"
                                    },
                                    "type": {
                                        "type": "string"
                                    },
                                    "cellPhone": {
                                        "type": "string"
                                    },
                                    "operatingTimes": {
                                        "type": "object",
                                        "properties": {
                                            "dayOfWeek": {
                                                "type": "string"
                                            },
                                            "openingTime": {
                                                "type": "string"
                                            },
                                            "closingTime": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                },
                                "example": {
                                    "name": "Giraffas",
                                    "cnpj": "28.713.241/0001-05",
                                    "type": "lanchonete",
                                    "cellPhone": "61992514712",
                                    "operatingTimes": {
                                        "dayOfWeek": "Domingo",
                                        "openingTime": "08:00:00",
                                        "closingTime": "22:30:00"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "name": "Giraffas",
                        "cnpj": "28.713.241/0001-05",
                        "type": "lanchonete",
                        "cellPhone": "61992514712",
                        "id": "cfc5dd59-3525-4d75-aa70-61ddfa6262cd"
                    },
                    "400": {
                        "description": "CNPJ already registred"
                    }
                }
            },
            "delete": {
                "tags": ["Restaurants"],
                "parameters": [{
                        "in": "path",
                        "name": "id",
                        "type": "string",
                        "required": true
                    }],
                "responses": {
                    "204": {
                        "description": "OK"
                    },
                    "500": {
                        "description": "internal server error"
                    }
                }
            }
        },
        "/isopen": {
            "post": {
                "tags": ["Comments"],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "description": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                    "name": "bbk",
                                    "date": "Domingo",
                                    "time": "17:00:00"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "O restaurante est√° aberto! :D"
                    }
                }
            }
        }
    }
}
